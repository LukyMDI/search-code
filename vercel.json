{
    "version": 2,
    "builds": [
        { "src": "src/backend/server/src/index.js", "use": "@vercel/node" },
        { "src": "src/frontend/*", "use": "@vercel/static" }
    ],
    "routes": [
        { "src": "/api/(.*)", "dest": "src/backend/server/src/index.js" },
        { "src": "/(.*)", "dest": "src/frontend/$1" }
    ]
}
